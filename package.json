{
  "name": "ai-ecommerce",
  "version": "1.0.0",
  "description": "AI-driven e-commerce platform",
  "private": true,
  "scripts": {
    "install:all": "npm install && npm install --prefix backend && npm install --prefix frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --prefix backend",
    "dev:frontend": "npm run start --prefix frontend",
    "build": "npm install --prefix frontend && npm run build --prefix frontend && npm install --prefix backend",
    "build:backend": "echo Backend is ready for production",
    "start": "npm start --prefix backend",
    "seed": "npm run seed --prefix backend",
    "test": "npm test --prefix frontend",
    "test-db": "npm run test-db --prefix backend",
    "setup-atlas": "npm run setup-atlas --prefix backend"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {},
  "keywords": [
    "ecommerce",
    "ai",
    "recommendations",
    "chatbot",
    "nodejs",
    "express",
    "react"
  ],
  "author": "",
  "license": "MIT"
}
